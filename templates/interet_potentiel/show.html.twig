{% extends 'template.html.twig' %}

{% block PageTitle %}
	Gestion des Interêts Potentiels des Partenaires
{% endblock %}

{% block title %}InteretPotentiel{% endblock %}

{% block body %}
<h5>Detail de l'interêt potentiel du partenaire</h5>

<table class="table table-bordered" id="dataTable" width="100%" cellspacing="0">

        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ interet_potentiel.id }}</td>
            </tr>
            <tr>
                <th>Interet</th>
                <td>{{ interet_potentiel.Interet }}</td>
            </tr>
        </tbody>
    </table>
<div class="d-flex flex-row">

    <a class="btn btn-outline-info btn-sm" href="{{ path('app_interet_potentiel_index') }}">
        <i class="bi bi-x-squarebi bi-x-square" style="font-size: 1.1rem;"></i>
    </a>

    <a class="btn btn-outline-primary btn-sm" href="{{ path('app_interet_potentiel_edit', {'id': interet_potentiel.id}) }}">
        <i class="bi bi-pencil-square" style="font-size: 1.1rem;"></i>
    </a>
    {{ include('interet_potentiel/_delete_form.html.twig') }}
</div>

{% endblock %}
