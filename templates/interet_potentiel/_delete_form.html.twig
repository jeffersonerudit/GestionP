<form method="post" action="{{ path('app_interet_potentiel_delete', {'id': interet_potentiel.id}) }}" onsubmit="return confirm('Voulez-vous vraiment supprimer ce statut de personne : {{ interet_potentiel.Interet }}?');">

    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ interet_potentiel.id) }}">
<button class="btn btn-outline-danger btn-sm">
