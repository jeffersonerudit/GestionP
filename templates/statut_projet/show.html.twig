{% extends 'template.html.twig' %}

{% block PageTitle %}
	Gestion de Statut de Projet
{% endblock %}


{% block title %}StatutProjet{% endblock %}

{% block body %}
<h5>Detail du statut de projet</h5>

<table class="table table-bordered" id="dataTable" width="100%" cellspacing="0">

        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ statut_projet.id }}</td>
            </tr>
            <tr>
                <th>Statut_P</th>
                <td>{{ statut_projet.StatutP }}</td>
            </tr>
        </tbody>
    </table>
<div class="d-flex flex-row">

    <a class="btn btn-outline-info btn-sm" href="{{ path('app_statut_projet_index') }}">
        <i class="bi bi-x-squarebi bi-x-square" style="font-size: 1.1rem;"></i>
    </a>

    <a class="btn btn-outline-primary btn-sm" href="{{ path('app_statut_projet_edit', {'id': statut_projet.id}) }}">
        <i class="bi bi-pencil-square" style="font-size: 1.1rem;"></i>
    </a>

    {{ include('statut_projet/_delete_form.html.twig') }}
    </div>
{% endblock %}
