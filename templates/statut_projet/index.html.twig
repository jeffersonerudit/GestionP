{% extends 'base.html.twig' %}

{% block title %}StatutProjet index{% endblock %}

{% block body %}
    <h1>StatutProjet index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Statut_P</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for statut_projet in statut_projets %}
            <tr>
                <td>{{ statut_projet.id }}</td>
                <td>{{ statut_projet.StatutP }}</td>
                <td>
                    <a href="{{ path('app_statut_projet_show', {'id': statut_projet.id}) }}">show</a>
                    <a href="{{ path('app_statut_projet_edit', {'id': statut_projet.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="3">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_statut_projet_new') }}">Create new</a>
{% endblock %}
